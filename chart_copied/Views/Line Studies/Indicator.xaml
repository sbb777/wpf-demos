<democommon:DemoControl
    x:Class="syncfusion.chartdemos.wpf.Indicator"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:democommon="clr-namespace:syncfusion.demoscommon.wpf;assembly=syncfusion.demoscommon.wpf"
    xmlns:local="clr-namespace:syncfusion.chartdemos.wpf"
    xmlns:skin="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    OptionsSize="Auto">

    <democommon:DemoControl.Options>
        <ComboBox
            x:Name="Indicators"
            Width="180"
            Height="20"
            Margin="5"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            IsEditable="False" SelectionChanged="OnIndicatorsSelectionChanged"/>
    </democommon:DemoControl.Options>
    <Grid x:Name="Grid">
        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="../../Resources/ColorModel.xaml" />
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Grid.Resources>

        <syncfusion:SfChart
            x:Name="sfchart"
            Margin="10"
            IsManipulationEnabled="True"
            Header="Google Stock Price">

            <syncfusion:SfChart.PrimaryAxis>
                <syncfusion:CategoryAxis
                    Header="Month"
                    LabelFormat="MMM/yy" FontFamily="Seoge UI" 
                    ShowGridLines="False"/>
            </syncfusion:SfChart.PrimaryAxis>

            <syncfusion:SfChart.SecondaryAxis>
                <syncfusion:NumericalAxis
                    OpposedPosition="True" LabelFormat="$0" Header="Price in USD" FontFamily="Seoge UI"  
                    ShowGridLines="True" />
            </syncfusion:SfChart.SecondaryAxis>

            <syncfusion:FastHiLoOpenCloseBitmapSeries
                x:Name="Series"
                Close="Last"
                High="High"
                ItemsSource="{Binding Datas}"
                Low="Low"
                Open="Open"
                XBindingPath="TimeStamp" />

        </syncfusion:SfChart>

    </Grid>
</democommon:DemoControl>
